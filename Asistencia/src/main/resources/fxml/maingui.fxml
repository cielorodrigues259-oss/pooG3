      <MenuBar fx:id="menuBar" BorderPane.alignment="CENTER">
        <menus>
          <Menu fx:id="menut" mnemonicParsing="false" text="eventos">
            <items>
              <MenuItem fx:id="menuItem1" mnemonicParsing="false" text="adm  .Participante" />
                  <MenuItem fx:id="menuItem2" mnemonicParsing="false" text="close" />
            </items>
          </Menu>
          <Menu  mnemonicParsing="false" text="Edit">
            <items>
